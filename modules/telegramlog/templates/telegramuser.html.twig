{#
/**
 * @file telegramuser.html.twig
 * Default theme implementation to present Telegram User data.
 *
 * This template is used when viewing Telegram User pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_telegramuser()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('telegramuser') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
